{
	"name": "Facebook Scroll to Top",
	"version": "0.1a",
	"manifest_version": 2,
	"description": "Scroll back to the top of your facebook pages.",
	"content_scripts": [
		{
			"matches": ["https://www.facebook.com/*"],
			"css": ["button.css"],
			"js": ["jquery-1.9.1.min.js", "scroll-button.js"]
		}
	]
}
